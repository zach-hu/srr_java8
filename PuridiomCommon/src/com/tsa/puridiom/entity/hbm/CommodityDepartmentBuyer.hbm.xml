        name="icHeader"
        type="java.math.BigDecimal"
        column="IC_HEADER"
    >
        <generator class="assigned" />
    </id>
    <property
        name="commodity"
        type="java.lang.String"
        column="COMMODITY"
        length="15"
    />
    <property
        name="departmentCode"
        type="java.lang.String"
        column="DEPARTMENT_CODE"
        length="15"
    />
	<property
        name="userId"
        type="java.lang.String"
        column="USER_ID"
        length="15"
    />

</class>

