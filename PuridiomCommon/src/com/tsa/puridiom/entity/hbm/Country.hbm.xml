<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

<!--
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class
    name="com.tsa.puridiom.entity.Country"
    table="COUNTRY"
>
    <id
        name="countryCode"
        type="java.lang.String"
        column="COUNTRY_CODE"
    >
        <generator class="assigned" />
    </id>
    <property
        name="countryName"
        type="java.lang.String"
        column="COUNTRY_NAME"
        length="255"
    />
    <property
        name="currencyCode"
        type="java.lang.String"
        column="CURRENCY_CODE"
        length="15"
    />
    <property
        name="timeZone"
        type="java.lang.String"
        column="TIME_ZONE"
        length="3"
    />
    <property
        name="dateEntered"
        type="java.sql.Timestamp"
        column="DATE_ENTERED"
        length="7"
    />
    <property
        name="dateExpires"
        type="java.sql.Timestamp"
        column="DATE_EXPIRES"
        length="7"
    />
    <property
        name="status"
        type="java.lang.String"
        column="STATUS"
        length="4"
    />
    <property
        name="owner"
        type="java.lang.String"
        column="OWNER"
        length="15"
    />
    <property
        name="dateFormat"
        type="java.lang.String"
        column="DATE_FORMAT"
        length="10"
    />
    <property
        name="language"
        type="java.lang.String"
        column="LANGUAGE"
        length="15"
    />
    <!-- associations -->

</class>
</hibernate-mapping>
