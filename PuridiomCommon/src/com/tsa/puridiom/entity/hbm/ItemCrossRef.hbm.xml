<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

<class
    name="com.tsa.puridiom.entity.ItemCrossRef"
    table="ITEM_CROSS_REF"
>
    <id
        name="altItemNumber"
        type="java.lang.String"
        column="ALT_ITEM_NUMBER"
    >
        <generator class="assigned" />
    </id>
    <property
        name="description"
        type="java.lang.String"
        column="DESCRIPTION"
        length="2000"
    />
    <property
        name="source"
        type="java.lang.String"
        column="SOURCE"
        length="20"
    />
    <property
        name="itemNumber"
        type="java.lang.String"
        column="ITEM_NUMBER"
        length="30"
    />
    <property
        name="dateEntered"
        type="java.sql.Timestamp"
        column="DATE_ENTERED"
        length="7"
    />
    <property
        name="dateExpires"
        type="java.sql.Timestamp"
        column="DATE_EXPIRES"
        length="7"
    />
    <property
        name="status"
        type="java.lang.String"
        column="STATUS"
        length="4"
    />
    <property
        name="owner"
        type="java.lang.String"
        column="OWNER"
        length="15"
    />
    <!-- associations -->

</class>
</hibernate-mapping>
