		<AssetLocation_id_sequenceNo>
			<label>routingSequence</label>
			<visible>1</visible>
		</AssetLocation_id_sequenceNo>
		<AssetLocation_locationType>
			<label>assetlocation-locationtype</label>
			<visible>1</visible>
		</AssetLocation_locationType>
		<AssetLocation_division>
			<label>assetlocationdivision</label>
			<visible>1</visible>
		</AssetLocation_division>
		<AssetLocation_department>
