<browse_commodity>
	<columns>
		<Commodity_commodity>
			<label>brw-commoditycode</label>
			<link><![CDATA[javascript: returnCommodity('#Commodity_commodity^'); void(0);]]></link>
			<size>15</size>
		</Commodity_commodity>
		<Commodity_description>
			<label>description</label>
			<sort>A</sort>
			<size>55</size>
			<filter-default>Y</filter-default>
		</Commodity_description>
		<Commodity_buyerCode>
			<label><![CDATA[brw-buyerName]]></label>
			<size>20</size>
            <type>USER-ID</type>
		</Commodity_buyerCode>
		<Commodity_status>
			<label>brw-status</label>
			<type>STATUS</type>
			<size>10</size>
		</Commodity_status>
	</columns>
<sqlfrom>Commodity as Commodity</sqlfrom>
<sqlwhere><![CDATA[ ]]></sqlwhere>
<sqlorderby>Commodity.description ASC</sqlorderby>
<pagesize>15</pagesize>
<title>Commodity Browse</title>
</browse_commodity>