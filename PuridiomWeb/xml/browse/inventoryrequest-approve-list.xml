<browse_requisition_header>
	<columns>
		<RequisitionHeader_icReqHeader>
	        <hidden>Y</hidden>
		</RequisitionHeader_icReqHeader>
		<RequisitionHeader_requisitionNumber>
			<label>requisitionNumber</label>
			<sort>D</sort>
			<link><![CDATA[javascript: viewInventoryReqPreview('#RequisitionHeader_icReqHeader^'); void(0);]]></link>
			<size>10</size>
		</RequisitionHeader_requisitionNumber>
		<RequisitionHeader_requisitionType>
			<label>poType</label>
			<type>REQUISITION-TYPE</type>
			<size>18</size>
		</RequisitionHeader_requisitionType>
		<RequisitionHeader_requisitionDate>
			<label>requisitionDate</label>
			<size>11</size>
			<type>DATE</type>
		</RequisitionHeader_requisitionDate>
		<RequisitionHeader_requisitionerCode>
			<label>requisitioner</label>
            <size>15</size>
            <type>USER-ID</type>
		</RequisitionHeader_requisitionerCode>
		<RequisitionHeader_vendorId>
			<label>brw-vendorId</label>
			<size>40</size>
			<detail>Y</detail>
		</RequisitionHeader_vendorId>
		<RequisitionHeader_status>
			<label>brw-status</label>
			<type>STATUS</type>
			<size>15</size>
		</RequisitionHeader_status>
		<RequisitionHeader_fiscalYear>
			<label>fiscalYear</label>
			<visible>0</visible>
			<allowfilter>Y</allowfilter>
		</RequisitionHeader_fiscalYear>
		<RequisitionHeader_dateEntered>
			<label>brw-dateentered</label>
			<hidden>Y</hidden>
			<type>DATE</type>
		</RequisitionHeader_dateEntered>
		<RequisitionHeader_requiredDate>
			<label>brw-requiredDate</label>
			<size>11</size>
			<type>DATE</type>
		</RequisitionHeader_requiredDate>
		<RequisitionHeader_total>
			<label>brw-total</label>
			<type>DollarDecimal</type>
			<size>15</size>
		</RequisitionHeader_total>
		<RequisitionHeader_approved>
			<label>appDate</label>
			<hidden>Y</hidden>
		</RequisitionHeader_approved>
		<RequisitionHeader_owner>
			<label>owner</label>
	        <hidden>Y</hidden>
			<allowfilter>Y</allowfilter>
		</RequisitionHeader_owner>
		<RequisitionHeader_departmentCode>
			<label>department</label>
	        <hidden>Y</hidden>
			<allowfilter>Y</allowfilter>
		</RequisitionHeader_departmentCode>
		<RequisitionHeader_shipToCode>
			<label>shipToCode</label>
	        <hidden>Y</hidden>
			<allowfilter>Y</allowfilter>
		</RequisitionHeader_shipToCode>
		<RequisitionHeader_vendorName>
			<label>brw-vendorName</label>
			<size>60</size>
			<detail>Y</detail>
		</RequisitionHeader_vendorName>
		<RequisitionHeader_buyer>
			<label>brw-suggestedBuyer</label>
			<detail>Y</detail>
			<size>40</size>
			<type>USER-ID</type>
		</RequisitionHeader_buyer>
		<RequisitionHeader_timeZone>
			<label>timezone</label>
			<detail>Y</detail>
			<size>60</size>
			<type>TIMEZONE</type>
		</RequisitionHeader_timeZone>
		<RequisitionHeader_internalComments>
			<label>req-purpose</label>
            <size>100</size>
            <detail>Y</detail>
		</RequisitionHeader_internalComments>
	</columns>
<sqlfrom>RequisitionHeader as RequisitionHeader</sqlfrom>
<sqlwhere><![CDATA[RequisitionHeader.requisitionNumber <> 'N/A'
					AND RequisitionHeader.requisitionType = 'S'
					AND RequisitionHeader.status = '1035' ]]></sqlwhere>
<sqlorderby>RequisitionHeader.requisitionNumber DESC</sqlorderby>
<pagesize>15</pagesize>
<title>Inventory Request Browse</title>
</browse_requisition_header>
