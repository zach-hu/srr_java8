<browse_requisition_header>
	<columns>
		<c_checkbox>
			<label>Select</label>
			<type>brw-checkbox</type>
			<size>5</size>
			<selectsql>N</selectsql>
			<store-request-value>Y</store-request-value>
		</c_checkbox>
		<RequisitionHeader_requisitionType>
			<label>poType</label>
			<type>REQUISITION-TYPE</type>
			<size>15</size>
		</RequisitionHeader_requisitionType>
		<RequisitionHeader_icReqHeader>
	        <hidden>Y</hidden>
			<hidden-input>Y</hidden-input>
			<key>Y</key>
		</RequisitionHeader_icReqHeader>
		<RequisitionHeader_requisitionNumber>
			<label>brw-requisitionNumber</label>
			<sort>D</sort>
			<link><![CDATA[javascript: viewMyOpenReq('#RequisitionHeader_icReqHeader^','#RequisitionHeader_requisitionType^'); void(0);]]></link>
			<size>10</size>
		</RequisitionHeader_requisitionNumber>
		<RequisitionHeader_requisitionDate>
			<label>brw-requisitionDate</label>
			<size>11</size>
			<type>DATE</type>
		</RequisitionHeader_requisitionDate>
		<RequisitionHeader_status>
			<label>brw-status</label>
			<type>STATUS</type>
			<size>15</size>
		</RequisitionHeader_status>
		<RequisitionHeader_owner>
			<label>owner</label>
	        <hidden>Y</hidden>
		</RequisitionHeader_owner>
		<RequisitionHeader_fiscalYear>
			<label>fiscalYear</label>
			<visible>0</visible>
		</RequisitionHeader_fiscalYear>
		<RequisitionHeader_dateEntered>
			<label>dateEntered</label>
			<hidden>Y</hidden>
			<type>DATE</type>
		</RequisitionHeader_dateEntered>
		<RequisitionHeader_requiredDate>
			<label>brw-requiredDate</label>
			<type>DATE</type>
			<size>11</size>
		</RequisitionHeader_requiredDate>
		<RequisitionHeader_total>
			<label>brw-total</label>
			<type>DollarDecimal</type>
			<size>10</size>
		</RequisitionHeader_total>
		<RequisitionHeader_approved>
			<label>routingApproved</label>
			<hidden>Y</hidden>
		</RequisitionHeader_approved>
		<RequisitionHeader_requisitionerCode>
			<label>brw-requisitionerCode</label>
            <size>15</size>
            <type>USER-ID</type>
			<hidden>Y</hidden>
		</RequisitionHeader_requisitionerCode>
		<RequisitionHeader_vendorId>
			<label>brw-vendorId</label>
			<size>40</size>
			<detail>Y</detail>
		</RequisitionHeader_vendorId>
		<RequisitionHeader_vendorName>
			<label>brw-vendorName</label>
			<size>60</size>
			<detail>Y</detail>
		</RequisitionHeader_vendorName>
		<RequisitionHeader_buyer>
			<label>suggestedBuyer</label>
			<detail>Y</detail>
			<size>100</size>
			<type>USER-ID</type>
		</RequisitionHeader_buyer>
		<RequisitionHeader_internalComments>
			<label>brw-purpose</label>
            <size>100</size>
            <detail>Y</detail>
		</RequisitionHeader_internalComments>
	</columns>
<sqlfrom>RequisitionHeader as RequisitionHeader</sqlfrom>
<sqlwhere><![CDATA[RequisitionHeader.requisitionNumber <> 'N/A' AND RequisitionHeader.requisitionerCode = :as_userid ]]></sqlwhere>
<sqlorderby>RequisitionHeader.requisitionNumber DESC</sqlorderby>
<pagesize>50</pagesize>
<title>Requisition Browse</title>
<detail-visible>Y</detail-visible>
</browse_requisition_header>
