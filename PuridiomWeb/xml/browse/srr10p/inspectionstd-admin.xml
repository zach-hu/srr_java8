<browse_inspectionstd>
	<columns>
		<InspectionStd_standardCode>
			<label>Standard Code</label>
			<link><![CDATA[javascript: returnInspectionStd('#InspectionStd_standardCode^'); void(0);]]></link>
			<size>15</size>
			<hidden-input>Y</hidden-input>
			<distinct>Y</distinct>
		</InspectionStd_standardCode>
		<InspectionStd_description>
			<label>brw-description</label>
			<hidden-input>Y</hidden-input>
			<size>60</size>
		</InspectionStd_description>
	</columns>
<sqlfrom>InspectionStd as InspectionStd</sqlfrom>
<sqlwhere><![CDATA[ InspectionStd.status = '02' or InspectionStd.status = '01' ]]></sqlwhere>
<sqlgroupby>InspectionStd.standardCode, InspectionStd.description</sqlgroupby>
<sqlorderby>InspectionStd.standardCode</sqlorderby>
<pagesize>15</pagesize>
<title>Inspection Standard Criteria</title>
</browse_inspectionstd>
