<browse_supercedes>
	<columns>
		<PoHeader_poType>
			<label>poType</label>
			<type>ORDER-TYPE</type>
			<size>15</size>
		</PoHeader_poType>
		<PoHeader_icPoHeader>
			<hidden-input>Y</hidden-input>
	        <hidden>Y</hidden>
	        <visible>0</visible>
		</PoHeader_icPoHeader>
		<PoHeader_poNumber>
			<label>brw-poNumber</label>
			<hidden-input>Y</hidden-input>
			<sort>D</sort>
			<size>15</size>
			<link><![CDATA[javascript: returnMe('#PoHeader_poNumber^'); void(0);]]></link>
		</PoHeader_poNumber>
		<PoHeader_releaseNumber>
			<hidden-input>Y</hidden-input>
			<label>brw-releaseNumber</label>
			<type>NoDecimal</type>
			<size>4</size>
		</PoHeader_releaseNumber>
		<PoHeader_revisionNumber>
			<hidden-input>Y</hidden-input>
			<label>brw-revisionNumber</label>
			<type>NoDecimal</type>
			<size>4</size>
		</PoHeader_revisionNumber>
		<PoHeader_status>
			<label>brw-status</label>
			<type>STATUS</type>
			<size>12</size>
		</PoHeader_status>
		<PoHeader_poDate>
			<label>brw-poDate</label>
			<type>DATE</type>
			<size>10</size>
		</PoHeader_poDate>
		<PoHeader_buyerCode>
			<label>brw-buyer</label>
            <size>12</size>
            <type>USER-ID</type>
		</PoHeader_buyerCode>
		<PoHeader_total>
			<label>brw-total</label>
			<size>10</size>
			<type>DollarDecimal</type>
		</PoHeader_total>
		<PoHeader_vendorId>
			<label>brw-vendorId</label>
			<detail>Y</detail>
			<size>25</size>
		</PoHeader_vendorId>
		<PoHeader_vendorName>
			<label>brw-vendorName</label>
			<detail>Y</detail>
			<size>50</size>
		</PoHeader_vendorName>
		<PoHeader_rfqNumber>
			<label>brw-solicitationNumber</label>
			<detail>Y</detail>
			<size>25</size>
			<hidden>Y</hidden>
		</PoHeader_rfqNumber>
		<PoHeader_requisitionNumber>
			<label>brw-requisitionNumber</label>
			<detail>Y</detail>
			<size>25</size>
		</PoHeader_requisitionNumber>
		<PoHeader_requisitionerCode>
			<label>requisitioner</label>
			<detail>Y</detail>
			<size>50</size>
			<type>USER-ID</type>
		</PoHeader_requisitionerCode>
		<PoHeader_departmentCode>
			<label>department</label>
			<detail>Y</detail>
			<size>25</size>
		</PoHeader_departmentCode>
		<PoHeader_internalComments>
			<label>brw-purpose</label>
			<detail>Y</detail>
			<size>100</size>
		</PoHeader_internalComments>
		<PoHeader_expirationDate>
			<label>dateExpires</label>
			<type>DATE</type>
			<size>10</size>
			<visible>0</visible>
		</PoHeader_expirationDate>
		<PoHeader_owner>
			<label>owner</label>
			<detail>Y</detail>
            <type>USER-ID</type>
		</PoHeader_owner>
		<PoHeader_requiredDate>
			<label>brw-requiredDate</label>
			<type>DATE</type>
			<hidden>Y</hidden>
		</PoHeader_requiredDate>
	</columns>
	<sqlfrom>PoHeader as PoHeader</sqlfrom>
	<sqlwhere><![CDATA[PoHeader.status > '3020' and PoHeader.status < '9020']]></sqlwhere>
	<sqlorderby>PoHeader.poNumber,PoHeader.lastRevision, PoHeader.releaseNumber, PoHeader.icPoHeader</sqlorderby>
	<pagesize>15</pagesize>
	<title>Purchasing Browse</title>
</browse_supercedes>