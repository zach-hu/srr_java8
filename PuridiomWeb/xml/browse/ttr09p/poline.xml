<browse_po_line>
	<columns>
		<PoLine_icPoHeader>
			<hidden>Y</hidden>
			<hidden-input>Y</hidden-input>
		</PoLine_icPoHeader>
		<PoLine_icPoLine>
			<hidden>Y</hidden>
			<hidden-input>Y</hidden-input>
		</PoLine_icPoLine>
		<!--PoLine_lineNumber>
			<label>Line</label>
		</PoLine_lineNumber-->
		<PoLine_poNumber>
			<label>brw-poNumber</label>
			<link><![CDATA[javascript: viewPo('#PoLine_icPoHeader^','#PoHeader_poType^'); void(0);]]></link>
			<size>10</size>
		</PoLine_poNumber>
		<PoLine_releaseNumber>
			<label>brw-releaseNumber</label>
			<type>STRING</type>
			<size>5</size>
		</PoLine_releaseNumber>
		<PoLine_itemNumber>
			<label>brw-itemNumber</label>
			<size>15</size>
		</PoLine_itemNumber>
		<PoLine_description>
			<label>description</label>
			<size>45</size>
			<trim>250</trim>
		</PoLine_description>
		<PoLine_status>
			<label>brw-status</label>
			<type>STATUS</type>
			<size>15</size>
		</PoLine_status>
		<PoLine_lineTotal>
			<label>brw-extendedCost</label>
			<type>DollarDecimal</type>
			<size>15</size>
		</PoLine_lineTotal>
		<PoLine_quantity>
			<hidden>Y</hidden>
			<type>QtyDecimal</type>
		</PoLine_quantity>
		<PoLine_umCode>
			<hidden>Y</hidden>
			<hidden-input>Y</hidden-input>
		</PoLine_umCode>
		<PoLine_quantity>
			<concatenate>
				<col type="QtyDecimal">PoLine_quantity</col>
				<col type="constant"><![CDATA[&nbsp;]]></col>
				<col type="constant">(</col>
				<col>PoLine_umCode</col>
				<col type="constant">)</col>
			</concatenate>
			<label>quantity</label>
			<type>Decimal</type>
			<size>25</size>
			<detail>Y</detail>
			<selectsql>N</selectsql>
		</PoLine_quantity>
		<PoLine_unitPrice>
			<label>brw-unitPrice</label>
			<type>DollarDecimal</type>
			<size>25</size>
			<detail>Y</detail>
		</PoLine_unitPrice>
		<PoLine_commodity>
			<label>commodity</label>
			<size>50</size>
			<detail>Y</detail>
		</PoLine_commodity>
		<!--PoLine_catalogId>
			<label>Catalog</label>
		</PoLine_catalogId>
		<PoLine_modelNumber>
			<label>Model Number</label>
		</PoLine_modelNumber>
		<PoLine_mfgName>
			<label>Mfg Name</label>
		</PoLine_mfgName-->
		<PoHeader_vendorId>
			<label>brw-vendorId</label>
			<detail>Y</detail>
			<size>25</size>
		</PoHeader_vendorId>
		<PoHeader_vendorName>
			<label>brw-vendorName</label>
			<detail>Y</detail>
			<size>50</size>
		</PoHeader_vendorName>
		<PoLine_requisitionerCode>
			<label>requisitionerCode</label>
			<size>25</size>
			<type>USER-ID</type>
			<detail>Y</detail>
		</PoLine_requisitionerCode>
		<PoHeader_udf5Code>
			<label><![CDATA[brw-po05]]></label>
			<detail>Y</detail>
			<size>25</size>
		</PoHeader_udf5Code>
		<PoHeader_udf7Code>
			<label><![CDATA[brw-po07]]></label>
			<detail>Y</detail>
			<size>75</size>
		</PoHeader_udf7Code>
		<PoHeader_fiscalYear>
			<label>brw-fiscalYear</label>
			<hidden>Y</hidden>
		</PoHeader_fiscalYear>
		<PoHeader_poDate>
			<label>brw-poDate</label>
			<hidden>Y</hidden>
		</PoHeader_poDate>
	</columns>
	<sqlfrom>PoLine as PoLine, PoHeader as PoHeader</sqlfrom>
	<sqlwhere><![CDATA[PoLine.poNumber <> 'N/A' AND PoHeader.icPoHeader = PoLine.icPoHeader AND PoHeader.lastRevision = 'C' ]]></sqlwhere>
	<sqlorderby>PoLine.poNumber DESC, PoLine.releaseNumber DESC</sqlorderby>
	<pagesize>15</pagesize>
	<title>Order Item Browse</title>
</browse_po_line>