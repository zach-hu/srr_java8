<?xml version="1.0" encoding="UTF-8"?>
<process name="DisbursementGetFormNumber" connection-type="Transaction-Required">
	<activities>
		<activity synchronous="true" continue-on-failure="false" name="getNumberSetup">
			<task synchronous="true" continue-on-failure="false" name="disbGetNumberSetup">
				<task-object-name>com.tsa.puridiom.disbursement.tasks.DisbursementGetNumberSetup</task-object-name>
			</task>
		</activity>
		<activity synchronous="true" continue-on-failure="false" name="autoGeneratedNumber">
			<task synchronous="true" continue-on-failure="false" name="disbGenerateNumber">
				<task-object-name>com.tsagate.foundation.processengine.InvokePuridiomProcessTask</task-object-name>
				<process-filename>disb-generate-form-number.xml</process-filename>
			</task>
		</activity>
		<activity synchronous="true" continue-on-failure="false" name="validateDisbNumber">
			<task synchronous="true" continue-on-failure="false" name="disbNumber">
				<task-object-name>com.tsa.puridiom.disbursement.tasks.DisbursementValidateNumber</task-object-name>
			</task>
		</activity>
		<activity synchronous="true" continue-on-failure="false" name="setDisbNumber">
			<task synchronous="true" continue-on-failure="false" name="disbHeader">
				<task-object-name>com.tsa.puridiom.disbursement.tasks.DisbursementSetNumber</task-object-name>
			</task>
			<task synchronous="true" continue-on-failure="false" name="disbLinesSetNumber">			
				<task-object-name>com.tsa.puridiom.disbline.tasks.DisbLineSetNumber</task-object-name>
			</task>
		</activity>
	</activities>
</process>