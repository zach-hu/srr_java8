<sqlfrom>PoHeader as PoHeader</sqlfrom>
<sqlwhere><![CDATA[PoHeader.poNumber <> 'N/A' AND PoHeader.status <> '9000' AND PoHeader.lastRevision = 'C' AND PoHeader.poType = 'BO']]></sqlwhere>
<pagesize>15</pagesize>
<title>Blanket Order Register Report</title>
</browse_blanketorder>