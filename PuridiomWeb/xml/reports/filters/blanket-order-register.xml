		<PoHeader_expirationDate>
			<label>Expiration Date</label>
			<type>DATE</type>
			<size>10</size>
		</PoHeader_expirationDate>
		<PoHeader_vendorId>
			<label>Supplier</label>
			<hidden>Y</hidden>
		</PoHeader_vendorId>
		<PoHeader_requiredDate>
			<label>Required Date</label>
			<type>DATE</type>
			<size>10</size>
		</PoHeader_requiredDate>
