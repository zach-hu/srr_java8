		<PoHeader_releaseLimit>
			<label>Release Limit</label>
			<type>DollarDecimal</type>
		</PoHeader_releaseLimit>
		<PoHeader_total>
			<label>Total</label>
			<type>DollarDecimal</type>
		</PoHeader_total>
		<PoHeader_currencyFactor>
			<label>Currency</label>
			<type>BigDecimal</type>
		</PoHeader_currencyFactor>
		<PoHeader_shippingTax>
			<label>Shipping Tax</label>
			<type>QtyDecimal</type>
