			<type>DATE</type>
			<size>10</size>
		</PoHeader_appDate>
		<PoHeader_vendorName>
			<label>Supplier Name</label>
			<size>15</size>
		</PoHeader_vendorName>
		<PoHeader_internalComments>
			<label>Purpose</label>
			<size>60</size>
		</PoHeader_internalComments>
