<browse_blanketorder>
	<columns>
		<PoHeader_contractNo>
			<label>Contract No.</label>
			<size>20</size>
		</PoHeader_contractNo>
		<PoHeader_poNumber>
			<label>PO #</label>
			<size>10</size>
		</PoHeader_poNumber>
		<PoHeader_releaseNumber>
			<label>Release</label>
			<type>BIGDECIMAL</type>
			<size>4</size>
		</PoHeader_releaseNumber>
		<PoHeader_revisionNumber>
			<label>Revision</label>
			<type>STRING</type>
			<size>4</size>
		</PoHeader_revisionNumber>
		<PoHeader_poType>
			<label>Type</label>
			<type>ORDER-TYPE</type>
			<size>15</size>
		</PoHeader_poType>
		<PoHeader_poDate>
			<label>PO Date</label>
			<type>DATE</type>
			<size>10</size>
