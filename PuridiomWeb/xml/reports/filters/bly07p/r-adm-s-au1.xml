<r-adm-s-au1>
	<columns>
		<UserProfile_userId>
			<label>User ID</label>
			<type>STRING</type>
		</UserProfile_userId>
		<UserProfile_firstName>
			<label>First Name</label>
			<type>STRING</type>
		</UserProfile_firstName>
		<UserProfile_lastName>
			<label>Last Name</label>
			<type>STRING</type>
		</UserProfile_lastName>
		<UserProfile_approvalAmount>
			<label>Amount</label>
			<type>BIGDECIMAL</type>
		</UserProfile_approvalAmount>
		<UserProfile_owner>
			<label>Owner</label>
			<type>STRING</type>
		</UserProfile_owner>
		<UserProfile_title>
			<label>Title</label>
			<type>STRING</type>
		</UserProfile_title>
		<AppRule_id_udf1Code>
			<label>Cost Center</label>
			<type>STRING</type>
		</AppRule_id_udf1Code>
		<AppRule_id_udf2Code>
			<label>Budgeted</label>
			<type>STRING</type>
		</AppRule_id_udf2Code>
		<AppRule_id_udf3Code>
			<label>Capital</label>
			<type>STRING</type>
		</AppRule_id_udf3Code>
		<AppRule_id_udf4Code>
			<label>Designated Approver</label>
			<type>STRING</type>
		</AppRule_id_udf4Code>
		<UserProfile_callForward>
			<label>Call Forward</label>
			<type>STRING</type>
		</UserProfile_callForward>
	</columns>
<sqlfrom>UserProfile as UserProfile, AppRule as AppRule </sqlfrom>
<sqlwhere><![CDATA[UserProfile.userId = AppRule.id.userId]]></sqlwhere>
<sqlorderby>AppRule.id.udf1Code, UserProfile.approvalAmount</sqlorderby>
<!-- <title>Cost Center Approvers Report</title> -->
<title>Approver List By Cost Center</title>
</r-adm-s-au1>