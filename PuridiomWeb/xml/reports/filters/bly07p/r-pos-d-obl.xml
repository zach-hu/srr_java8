  <sqlfrom>PoHeader as PoHeader, Vendor as Vendor</sqlfrom>
  <sqlwhere><![CDATA[
  	( PoHeader.vendorId = Vendor.vendorId ) and ( PoHeader.poDate >= :as_baseReportDate ) and
  	( PoHeader.status > '3020' ) and
  	( PoHeader.status < '9020' ) and
  	(	( PoHeader.status = '3030' ) or
  		( PoHeader.status = '4005' ) or
  		( PoHeader.status = '4010' ) or
  		( PoHeader.status = '9010' )
  	)
  ]]></sqlwhere>
  <sqlorderby>PoHeader.poType</sqlorderby>
