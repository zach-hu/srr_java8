  <sqlfrom> PoHeader as PoHeader , VendorInsurance as VendorInsurance</sqlfrom>
  <sqlwhere>
  	<![CDATA[ ( VendorInsurance.icPoHeader = PoHeader.icPoHeader ) and ( PoHeader.status < '3000' ) AND ( PoHeader.poType = 'CT' ) AND ( PoHeader.contractNo LIKE '%C-01%' ) ]]> </sqlwhere>
  <sqlorderby>PoHeader.vendorName ASC, PoHeader.contractNo ASC, PoHeader.expirationDate ASC</sqlorderby>
  <title>MSC Compliance Report</title>
