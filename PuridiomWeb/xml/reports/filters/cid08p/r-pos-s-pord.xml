	<sqlfrom>PoHeader as PoHeader, PoLine as PoLine , Commodity as Commodity , Department as Department, Account as Account</sqlfrom>
	<sqlwhere><![CDATA[
		( PoHeader.icPoHeader = PoLine.icPoHeader ) and
		( PoLine.icPoHeader = Account.id.icHeader )  and
		( PoLine.icAccount = Account.id.icLine )  and
		( PoHeader.lastRevision = 'C' ) and ( PoHeader.poDate >= :as_baseReportDate ) and
		( PoHeader.status > '3020') and
		( PoHeader.status < '9020')and
		( PoLine.commodity = Commodity.commodity) and
		( PoHeader.departmentCode = Department.departmentCode)
		]]></sqlwhere>
	<sqlorderby>PoHeader.poNumber ASC, PoLine.lineNumber ASC</sqlorderby>
 	<title>Purchase Order Report (Detail)</title>
