<r-vch-ship>
  <columns>
		<InvoiceHeader_invoiceNumber>
			<label>Invoice Number</label>
			<type>STRING</type>
			<visible>1</visible>
		</InvoiceHeader_invoiceNumber>
		<InvoiceHeader_invoiceType>
			<label>Invoice Type</label>
			<type>STRING</type>
			<visible>1</visible>
		</InvoiceHeader_invoiceType>
		<InvoiceHeader_poNumber>
			<label>poNumber</label>
			<type>STRING</type>
			<visible>1</visible>
		</InvoiceHeader_poNumber>
		<InvoiceHeader_poRelease>
			<label>Order Release</label>
			<type>BIGDECIMAL</type>
			<visible>1</visible>
		</InvoiceHeader_poRelease>
		<PoHeader_revisionNumber>
			<label>Revision Number</label>
			<type>BIGDECIMAL</type>
			<visible>1</visible>
		</PoHeader_revisionNumber>
		<InvoiceHeader_invoiceDate>
			<label>Invoice Date</label>
			<type>DATE</type>
			<visible>1</visible>
		</InvoiceHeader_invoiceDate>
		<InvoiceHeader_enteredBy>
			<label>Entered By</label>
			<type>STRING</type>
			<visible>1</visible>
		</InvoiceHeader_enteredBy>
		<InvoiceHeader_vendorId>
			<label>Vendor Id</label>
			<type>STRING</type>
			<visible>1</visible>
		</InvoiceHeader_vendorId>
		<InvoiceHeader_invoiceTotal>
			<label>Invoice Total</label>
			<type>BIGDECIMAL</type>
			<visible>1</visible>
		</InvoiceHeader_invoiceTotal>
		<InvoiceHeader_currencyCode>
			<label>Currency Code</label>
			<type>STRING</type>
			<visible>1</visible>
		</InvoiceHeader_currencyCode>
		<InvoiceHeader_currencyFactor>
			<label>Currency Factor</label>
			<type>BIGDECIMAL</type>
			<visible>1</visible>
		</InvoiceHeader_currencyFactor>
		<InvoiceHeader_invoiceShipping>
			<label>Shipping Charges</label>
			<type>BIGDECIMAL</type>
			<visible>1</visible>
		</InvoiceHeader_invoiceShipping>

	</columns>
  <sqlfrom>InvoiceHeader as InvoiceHeader, PoHeader as PoHeader</sqlfrom>
  <sqlwhere><![CDATA[ (InvoiceHeader.status = '6010' ) and (PoHeader.icPoHeader = InvoiceHeader.icPoHeader) and (InvoiceHeader.invoiceNumber <> 'N/A')]]></sqlwhere>
  <title>Voucher Shipping Charge Report</title>
</r-vch-ship>

