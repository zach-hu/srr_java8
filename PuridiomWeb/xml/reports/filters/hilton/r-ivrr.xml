<r-ivrr>
  <columns>
		<InvItem_commodity>
			<label>commodity</label>
			<type>STRING</type>
		</InvItem_commodity>
		<InvItem_description>
			<label>Description</label>
			<type>STRING</type>
		</InvItem_description>
		<PoLine_description>
			<label>description</label>
			<type>STRING</type>
		</PoLine_description>
		<ReceiptLine_receiptDate>
			<label>receiptDate</label>
			<type>DATE</type>
		</ReceiptLine_receiptDate>
		<PoLine_poNumber>
			<label>Po Number</label>
			<type>STRING</type>
		</PoLine_poNumber>
		<ReceiptLine_qtyReceived>
			<label>qty Received</label>
			<type>BIGDECIMAL</type>
		</ReceiptLine_qtyReceived>
		<ReceiptLine_qtyReturned>
			<label>Quantity Returned</label>
			<type>BIGDECIMAL</type>
		</ReceiptLine_qtyReturned>
		<ReceiptLine_qtyRejected>
			<label>Quantity Rejected</label>
			<type>BIGDECIMAL</type>
		</ReceiptLine_qtyRejected>
		<InvBinLocation_aisle>
			<label>aisle</label>
			<type>STRING</type>
		</InvBinLocation_aisle>
		<InvBinLocation_locrow>
			<label>Locrow</label>
			<type>STRING</type>
		</InvBinLocation_locrow>
		<InvBinLocation_tier>
			<label>tier</label>
			<type>STRING</type>
		</InvBinLocation_tier>
		<InvBinLocation_bin>
			<label>bin</label>
			<type>STRING</type>
		</InvBinLocation_bin>
		<InvBinLocation_itemLocation>
			<label>location</label>
			<type>STRING</type>
		</InvBinLocation_itemLocation>
		<PoLine_itemNumber>
			<label>Item Number</label>
			<type>STRING</type>
		</PoLine_itemNumber>
		<PoLine_unitPrice>
			<label>Unit Price</label>
			<type>BIGDECIMAL</type>
		</PoLine_unitPrice>
		<PoLine_releaseNumber>
			<label>Release Number</label>
			<type>BIGDECIMAL</type>
		</PoLine_releaseNumber>
		<PoHeader_vendorName>
			<label>Vendor Name</label>
			<type>STRING</type>
		</PoHeader_vendorName>
		<PoHeader_shipToCode>
			<label>Ship To Code</label>
			<type>STRING</type>
		</PoHeader_shipToCode>
	</columns>
  <sqlfrom> InvBinLocation as InvBinLocation , ReceiptLine as ReceiptLine , InvItem as InvItem , PoLine as PoLine , PoHeader as PoHeader </sqlfrom>
  <sqlwhere><![CDATA[(InvItem.itemNumber = InvBinLocation.itemNumber) and (InvItem.itemNumber = PoLine.itemNumber) and (PoLine.icPoLine = ReceiptLine.icPoLine ) and (PoLine.icPoHeader = PoHeader.icPoHeader)]]></sqlwhere>
  <title>Inventory Receipt Report (Extended Inventory)</title>
</r-ivrr>