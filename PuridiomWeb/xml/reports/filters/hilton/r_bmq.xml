<r_bmq>
  <columns>
		<InvItem_description>
			<label>inv-description</label>
			<type>STRING</type>
		</InvItem_description>
		<InvItem_itemNumber>
			<label>inv-itemNumber</label>
			<type>STRING</type>
		</InvItem_itemNumber>
		<InvLocation_qtyOnHand>
			<label>locbrw-inv-quantityOnHand</label>
			<type>BIGDECIMAL</type>
		</InvLocation_qtyOnHand>
		<InvLocation_minOnHand>
			<label>locbrw-inv-minimumOnHand</label>
			<type>BIGDECIMAL</type>
		</InvLocation_minOnHand>
		<InvLocation_qtyEoq>
			<label>locbrw-inv-economicOrderQuantity</label>
			<type>BIGDECIMAL</type>
		</InvLocation_qtyEoq>
		<InvItem_udf1Code>
			<label>inv-Udf1Code</label>
			<type>STRING</type>
		</InvItem_udf1Code>
		<InvItem_udf2Code>
			<label>inv-Udf2Code</label>
			<type>STRING</type>
		</InvItem_udf2Code>
		<InvItem_udf3Code>
			<label>inv-Udf3Code</label>
			<type>STRING</type>
		</InvItem_udf3Code>
		<InvItem_udf4Code>
			<label>inv-Udf4Code</label>
			<type>STRING</type>
		</InvItem_udf4Code>
		<InvItem_udf5Code>
			<label>inv-Udf5Code</label>
			<type>STRING</type>
		</InvItem_udf5Code>
		<InvLocation_qtyOnOrder>
			<label>locbrw-inv-quantityOnOrder</label>
			<type>BIGDECIMAL</type>
		</InvLocation_qtyOnOrder>
	</columns>

  <sqlfrom> InvItem as InvItem , InvLocation as InvLocation </sqlfrom>
  <sqlwhere><![CDATA[ (InvItem.itemNumber = InvLocation.id.itemNumber) and (InvLocation.qtyOnHand < InvLocation.minOnHand) and ( InvLocation.qtyOnHand <> 0) and (InvItem.status <> '03' )]]></sqlwhere>
  <title>Below Min Quantity Report</title>
</r_bmq>

