<r-sup-d-nts>
	<columns>
		<PoHeader_total>
			<label>Total</label>
			<type>BIGDECIMAL</type>
			<visible>1</visible>
		</PoHeader_total>
		<PoHeader_vendorId>
			<label>Vendor Id</label>
			<type>STRING</type>
			<visible>1</visible>
		</PoHeader_vendorId>
		<PoHeader_poDate>
			<label>Order Date</label>
			<selectsql>N</selectsql>
			<type>DATE</type>
			<visible>1</visible>
		</PoHeader_poDate>
		<PoHeader_currencyCode>
			<label>currencyCode</label>
			<type>STRING</type>
			<visible>0</visible>
		</PoHeader_currencyCode>
	</columns>
<sqlfrom>PoHeader as PoHeader</sqlfrom>
<sqlwhere><![CDATA[PoHeader.status >= '3030' and PoHeader.status < '9000' and PoHeader.lastRevision = 'C' AND PoHeader.vendorId IS NOT NULL ]]></sqlwhere>
<title>New Top Suppliers Report</title>
</r-sup-d-nts>

