			<type>STRING</type>
		</Account_allocAmount>
	</columns>
	<sqlfrom>PoHeader as PoHeader, Account as Account, Vendor as Vendor</sqlfrom>
  <sqlwhere><![CDATA[( PoHeader.vendorId = Vendor.vendorCode ) and ( PoHeader.icPoHeader = Accounts.headerIc ) and  ( ( PoHeader.status >= '3030' ) AND  ( PoHeader.status < '9000' ) AND( PoHeader.approved = 'Y' ) ) ]]></sqlwhere>
  <title>Po Obligation Report</title>
</poobligation>

