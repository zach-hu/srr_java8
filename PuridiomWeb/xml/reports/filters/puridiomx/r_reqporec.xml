  <sqlfrom> PoHeader as PoHeader, UserProfile as UserProfile, RequisitionHeader as RequisitionHeader, RfqHeader as RfqHeader, Vendor as Vendor </sqlfrom>
  <sqlwhere><![CDATA[
  	( PoHeader.icReqHeader = RequisitionHeader.icReqHeader ) and
  	( PoHeader.icRfqHeader = RfqHeader.icRfqHeader ) and
  	( PoHeader.vendorId = Vendor.vendorId ) and ( PoHeader.poDate >= :as_baseReportDate ) and
  	( PoHeader.status > '3020' ) and
  	( PoHeader.status < '9020' )
  	]]></sqlwhere>
  <title>Total Procurement Time</title>
