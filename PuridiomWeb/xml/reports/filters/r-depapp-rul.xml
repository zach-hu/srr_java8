<r-depapp-rul>
	<columns>
		<ApprovalLog_id_userId>
			<label>user Id</label>
			<type>STRING</type>
		</ApprovalLog_id_userId>
		<ApprovalLog_callForward>
			<label>Call Forward</label>
			<type>STRING</type>
		</ApprovalLog_callForward>
		<AppRulesExt_ruleAction>
			<label>Rule Action</label>
			<type>STRING</type>
		</AppRulesExt_ruleAction>
		<AppRulesExt_routto>
			<label>Rout To</label>
			<type>STRING</type>
		</AppRulesExt_routto>
	</columns>
<sqlfrom> ApprovalLog as ApprovalLog, AppRulesExt as AppRulesExt </sqlfrom>
<sqlwhere><![CDATA[(ApprovalLog.ruleAction = AppRulesExt.ruleAction )]]></sqlwhere>
<title>Department Approval Routing</title>
</r-depapp-rul>