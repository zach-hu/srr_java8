  <sqlwhere> <![CDATA[(PoHeader.icReqHeader = RequisitionHeader.icReqHeader)and((PoHeader.icPoHeader = InvoiceHeader.icPoHeader)or(InvoiceHeader.icPoHeader is not null)) and (PoHeader.icPoHeader = Account.id.icHeader) and (PoHeader.lastRevision = 'C') and ( PoHeader.status > '3000' ) and (PoHeader.status < '9020')]]> </sqlwhere>
