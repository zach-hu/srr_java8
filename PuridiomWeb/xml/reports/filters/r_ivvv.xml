		<InvItem_description>
			<label>Description</label>
			<type>STRING</type>
		</InvItem_description>
		<InvItem_commodity>
			<label>Commodity</label>
			<type>STRING</type>
		</InvItem_commodity>
		<InvBinLocation_itemLocation>
			<label>Item Location</label>
			<type>STRING</type>
