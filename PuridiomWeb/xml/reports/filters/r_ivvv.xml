		<InvBinLocation_manufactNo>
			<label>Manufact No</label>
			<type>STRING</type>
		</InvBinLocation_manufactNo>
		<InvBinLocation_lot>
			<label>Lot</label>
			<type>STRING</type>
		</InvBinLocation_lot>
		<InvBinLocation_cost>
			<label>Cost</label>
			<type>BIGDECIMAL</type>
		</InvBinLocation_cost>
