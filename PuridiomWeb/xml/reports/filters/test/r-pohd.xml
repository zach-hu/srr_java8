	</columns>
	<sqlfrom>PoHeader as PoHeader, PoLine as PoLine, Account as Account, UserProfile as UserProfile</sqlfrom>
	<sqlwhere><![CDATA[( PoHeader.requisitionerCode = UserProfile.userId (+) ) and ( PoHeader.icPoHeader = PoLine.icPoHeader ) and ( PoLine.icPoHeader = Account.id.icHeader ) and ( PoLine.icAccount = Account.id.icLine ) and ( PoHeader.lastRevision = 'C' ) and ( PoHeader.poDate >= :as_baseReportDate ) and ( PoHeader.status > '3020' ) and ( PoHeader.status < '9020' )]]></sqlwhere>
	<sqlgroupby>PoHeader.icPoHeader, PoLine.icPoLine, PoHeader.poNumber, PoHeader.releaseNumber, PoLine.lineNumber</sqlgroupby>
	<sqlorderby>max(fld_2), PoHeader.poNumber, PoHeader.releaseNumber, PoLine.lineNumber </sqlorderby>
	<title>PO Detail By Cost Center Report</title>
</r-pohd>

