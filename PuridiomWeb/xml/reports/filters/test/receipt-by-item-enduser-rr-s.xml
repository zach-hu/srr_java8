<browse-receipt-by-item-enduser-rr>
<columns>
	<PoHeader_poType>
		<label>Type</label>
		<type>ORDER-TYPE</type>
		<size>14</size>
	</PoHeader_poType>
	<PoHeader_icPoHeader>
		<label>Ic Po Header</label>
		<hidden>Y</hidden>
		<visible>0</visible>
	</PoHeader_icPoHeader> 
	<PoHeader_poNumber>
		<label>PO #</label>
		<link><![CDATA[javascript: receiveByOrder('#PoHeader_icPoHeader^'); void(0);]]></link>
		<size>12</size>
	</PoHeader_poNumber>
	<PoHeader_releaseNumber>
		<label>Rel #</label>
		<type>STRING</type>
		<size>5</size>
	</PoHeader_releaseNumber>
	<PoHeader_revisionNumber>
		<label>Rev #</label>
		<type>STRING</type>
		<hidden>Y</hidden>
	</PoHeader_revisionNumber>
	<PoHeader_status>
		<label>Status</label>
		<type>STATUS</type>
		<size>15</size>
	</PoHeader_status>
	<PoHeader_poDate>
		<label>PO Date</label>
		<type>DATE</type>
		<size>11</size>
	</PoHeader_poDate>
	<PoHeader_vendorId>
		<label>Supplier</label>
		<hidden>Y</hidden>
	</PoHeader_vendorId>
	<PoHeader_vendorName>
		<label>Supplier Name</label>
		<size>19</size>
	</PoHeader_vendorName>
	<PoHeader_buyerCode>
		<label>Buyer</label>
		<size>13</size>
		<type>USER-ID</type>
	</PoHeader_buyerCode>
	<PoHeader_requiredDate>
		<label>Required By</label>
		<type>DATE</type>
		<size>11</size>
	</PoHeader_requiredDate>
	<PoHeader_internalComments>
		<label>Purpose</label>
		<detail>Y</detail>
	</PoHeader_internalComments>
	<PoHeader_pyStatus>
		<label>Invoice Status</label>
		<type>STATUS</type>
		<detail>Y</detail>
	</PoHeader_pyStatus>
</columns>
<sqlfrom>PoHeader as PoHeader</sqlfrom>
<sqlwhere><![CDATA[PoHeader.poNumber is not null 
	and PoHeader.poNumber <> 'null' 
	and PoHeader.status >= '3030' 
	and PoHeader.status < '4010' 
	and PoHeader.status <> '3045' 
	and PoHeader.lastRevision = 'C'
	and PoHeader.poType <> 'BO'
	and PoHeader.poType <> 'DO'
	and PoHeader.poType <> 'SB'
	and PoHeader.icPoHeader in 
		(SELECT distinct PoHeader.icPoHeader 
			FROM PoHeader as PoHeader, PoLine as PoLine
			WHERE PoHeader.icPoHeader = PoLine.icPoHeader
				and PoLine.status >= '3030' 
				and PoLine.status < '4010' 
				and PoLine.status <> '3045' 
				and (PoLine.receiptRequired = 'E' or PoLine.receiptRequired = 'R'))
	]]></sqlwhere>
<sqlorderby>PoHeader.poDate DESC</sqlorderby>
<pagesize>15</pagesize>
<title>Order Browse for Receiving</title>
</browse-receipt-by-item-enduser-rr>