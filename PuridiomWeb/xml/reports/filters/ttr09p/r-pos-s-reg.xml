<r_pohs>
  <columns>
		<PoHeader_poNumber>
			<label>PO Number</label>
			<type>STRING</type>
		</PoHeader_poNumber>
		<PoHeader_releaseNumber>
			<label>Release Number</label>
			<type>BIGDECIMAL</type>
		</PoHeader_releaseNumber>
		<PoHeader_departmentCode>
			<label>Lab Code</label>
			<type>STRING</type>
		</PoHeader_departmentCode>
		<PoHeader_poDate>
			<label>PO Date</label>
			<type>DATE</type>
		</PoHeader_poDate>
		<PoHeader_requiredDate>
			<label>Required Date</label>
			<type>DATE</type>
		</PoHeader_requiredDate>
		<PoHeader_buyerCode>
			<label>Buyer Code</label>
			<type>STRING</type>
		</PoHeader_buyerCode>
		<PoHeader_requisitionNumber>
			<label>Requisition Number</label>
			<type>STRING</type>
		</PoHeader_requisitionNumber>
		<PoHeader_requisitionerCode>
			<label>Requisitioner Code</label>
			<type>STRING</type>
		</PoHeader_requisitionerCode>
		<PoHeader_vendorName>
			<label>Vendor Name</label>
			<type>STRING</type>
		</PoHeader_vendorName>
		<PoHeader_internalComments>
			<label>Purpose</label>
			<type>STRING</type>
		</PoHeader_internalComments>
		<PoHeader_status>
			<label>Status</label>
			<type>STATUS</type>
		</PoHeader_status>
		<PoHeader_total>
			<label>Total</label>
			<type>BIGDECIMAL</type>
		</PoHeader_total>
		<PoHeader_currencyCode>
			<label>Currency Code</label>
			<type>STRING</type>
		</PoHeader_currencyCode>
   </columns>
  <sqlfrom> PoHeader as PoHeader  </sqlfrom>
  <sqlwhere> <![CDATA[
  	( PoHeader.lastRevision = 'C' ) and ( PoHeader.poDate >= :as_baseReportDate ) and
  	( PoHeader.status > '3020' ) and
  	( PoHeader.status < '9020' ) ]]> </sqlwhere>
  <sqlorderby>PoHeader.poNumber, PoHeader.releaseNumber</sqlorderby>
  <title>Purchase Order Report (Summary)</title>
</r_pohs>

