<sqlfrom> InvoiceAddress as InvoiceAddress , InvoiceHeader as InvoiceHeader , Account as Account , InvoiceVendor as InvoiceVendor , InvoiceLine as InvoiceLine , PoHeader as PoHeader , Vendor as Vendor </sqlfrom>
<sqlwhere><![CDATA[
InvoiceHeader.icIvcHeader = Account.id.icHeader and
InvoiceLine.icIvcLine = Account.id.icLine and
InvoiceHeader.vendorId = InvoiceVendor.vendorId and
InvoiceVendor.vendorId = InvoiceAddress.id.vendorId and
InvoiceHeader.vendorAddrCode = InvoiceAddress.id.addressCode and
InvoiceHeader.icIvcHeader = InvoiceLine.icIvcHeader and
InvoiceHeader.icPoHeader = PoHeader.icPoHeader and
InvoiceHeader.vendorId = Vendor.vendorId
]]></sqlwhere>
<sqlorderby>InvoiceHeader.invoiceNumber ASC</sqlorderby>
