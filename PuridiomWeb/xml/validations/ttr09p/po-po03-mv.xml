<?xml version="1.0" encoding="UTF-8"?>
<rule>
	<clause type="StringCompare" matchAll="true">
		<left-side>
			<source>incomingRequestObjectMethod</source>
			<object>header</object>
			<name>getUdf1Code</name>
		</left-side>
		<operator>STARTSWITH</operator>
		<right-side>
			<source>constant</source>
			<value>RESALE</value>
		</right-side>
	</clause>
	<logical-operator>IFTHEN</logical-operator>
	<clause type="lookup">
		<sql>from StdTable stdTable where stdTable.id.tableType = 'PO03' AND stdTable.id.tableKey = '@udf3@' AND (stdTable.status = '02' OR (stdTable.status = '01' AND stdTable.dateExpires >= @today@))</sql>
		<source>header</source>
		<arguments>
			<name>udf3</name>
			<source>header</source>
			<colName>getUdf3Code</colName>
		</arguments>
	</clause>
</rule>