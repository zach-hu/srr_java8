<?xml version="1.0" encoding="UTF-8"?>
<validation-rules>
  <validations>
    <save enabled="yes">
		<rule>
			<filename>packages-and-qtyreceived-not-zero.xml</filename>
			<alias>Packages</alias>
			<msg>if # Packages is greater than 0, Quantity Received must not be 0!</msg>
			<severity>E</severity>
			<order>5</order>
			<enabled>Y</enabled>
			<new>true</new>
		</rule>
    </save>
  </validations>
</validation-rules>