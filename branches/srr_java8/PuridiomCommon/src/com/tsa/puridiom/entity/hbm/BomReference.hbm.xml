<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="com.tsa.puridiom.entity.BomReference" 
    table="BOM_REFERENCE"
>
    <id
        name="icReference"
        type="java.math.BigDecimal"
        column="IC_REFERENCE"
    >
        <generator class="assigned" />
    </id>
    <property
        name="parentItem"
        type="java.lang.String"
        column="PARENT_ITEM"
        length="30"
    />
    <property
        name="componentItem"
        type="java.lang.String"
        column="COMPONENT_ITEM"
        length="30"
    />
    <property
        name="icComponent"
        type="java.math.BigDecimal"
        column="IC_COMPONENT"
        length="22"
    />
    <property
        name="referenceId"
        type="java.lang.String"
        column="REFERENCE_ID"
        length="30"
    />
    <property
        name="refNo"
        type="java.math.BigDecimal"
        column="REF_NO"
        length="15"
    />
    <property
        name="qty"
        type="java.math.BigDecimal"
        column="QTY"
        length="15"
    />
    <property
        name="methodId"
        type="java.lang.String"
        column="METHOD_ID"
        length="15"
    />
    <property
        name="stageId"
        type="java.lang.String"
        column="STAGE_ID"
        length="15"
    />
    <property
        name="icRouting"
        type="java.math.BigDecimal"
        column="IC_ROUTING"
        length="22"
    />
    <property
        name="dateEntered"
        type="java.sql.Timestamp"
        column="DATE_ENTERED"
        length="7"
    />
    <property
        name="owner"
        type="java.lang.String"
        column="OWNER"
        length="15"
    />

    <!-- associations -->

</class>
</hibernate-mapping>
