        <key-property name="icMsrLine" column="IC_MSR_LINE" type="java.math.BigDecimal"/>
    </composite-id>
    <property
        name="inspectType"
        type="java.lang.String"
        column="INSPECT_TYPE"
        length="15"
    />
    <property
        name="inspectorId"
        type="java.lang.String"
        column="INSPECTOR_ID"
        length="15"
    />
    <property
        name="engineerId"
        type="java.lang.String"
        column="ENGINEER_ID"
        length="15"
    />
    <property
        name="remoteInspId"
        type="java.lang.String"
        column="REMOTE_INSP_ID"
        length="30"
    />
    <property
        name="purchaseSpecs"
        type="java.lang.String"
        column="PURCHASE_SPECS"
        length="40"
    />
    <property
        name="area"
        type="java.lang.String"
        column="AREA"
        length="15"
    />
    <property
        name="storage"
        type="java.lang.String"
        column="STORAGE"
        length="15"
    />
    <property
        name="location"
        type="java.lang.String"
        column="LOCATION"
        length="15"
    />
    <property
        name="standardCode"
        type="java.lang.String"
        column="STANDARD_CODE"
        length="15"
    />
    <property
        name="udf01"
        type="java.lang.String"
        column="UDF_01"
        length="15"
    />
    <property
        name="udf02"
        type="java.lang.String"
        column="UDF_02"
        length="30"
    />
    <property
        name="udf03"
        type="java.lang.String"
        column="UDF_03"
        length="15"
    />
    <property
        name="udf04"
        type="java.lang.String"
        column="UDF_04"
        length="40"
    />
    <property
        name="udf05"
        type="java.lang.String"
        column="UDF_05"
        length="15"
    />
    <property
        name="inspType"
        type="java.lang.String"
        column="INSP_TYPE"
        length="15"
    />
    <property
        name="apprDt"
        type="java.sql.Date"
        column="APPR_DT"
        length="7"
    />
    <property
        name="dateEntered"
        type="java.sql.Timestamp"
        column="DATE_ENTERED"
        length="7"
    />
    <property
        name="status"
        type="java.lang.String"
        column="STATUS"
        length="4"
    />
    <property
        name="owner"
        type="java.lang.String"
        column="OWNER"
        length="15"
    />
    <property
        name="lastChange"
        type="java.sql.Timestamp"
        column="LAST_CHANGE"
        length="7"
    />
    <property
        name="lastChangeBy"
        type="java.lang.String"
        column="LAST_CHANGE_BY"
        length="15"
    />
    <property
        name="assignedDate"
        type="java.sql.Timestamp"
        column="ASSIGNED_DATE"
        length="7"
    />
    <property
        name="icRecLine"
        type="java.math.BigDecimal"
        column="IC_REC_LINE"
        not-null="true"
        length="22"
    />
    <property
        name="poNumber"
        type="java.lang.String"
        column="PO_NUMBER"
        length="20"
    />
    <property
        name="icPoLine"
        type="java.math.BigDecimal"
        column="IC_PO_LINE"
        not-null="true"
        length="22"
    />
    <property
        name="shelfLifeDate"
        type="java.sql.Timestamp"
        column="SHELF_LIFE_DATE"
        length="7"
    />
    <property
        name="cgdNo"
        type="java.lang.String"
        column="CGD_NO"
        length="20"
    />
    <property
        name="cgdRev"
        type="java.math.BigDecimal"
        column="CGD_REV"
        length="5"
    />
 	<property
        name="inspectionNumber"
        type="java.lang.String"
        column="INSPECTION_NUMBER"
        length="30"
    />

    <property
        name="qtySignoff"
        type="java.math.BigDecimal"
        column="QTY_SIGNOFF"
        length="15"
    />
    <property
        name="qtyAccepted"
        type="java.math.BigDecimal"
        column="QTY_ACCEPTED"
        length="15"
    />
    <property
        name="qtyRejected"
        type="java.math.BigDecimal"
        column="QTY_REJECTED"
        length="15"
    />
    <property
        name="internalComments"
        type="java.lang.String"
        column="INTERNAL_COMMENTS"
        length="255"
    />

