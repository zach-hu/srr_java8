<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="com.tsa.puridiom.entity.InvManufacturer" 
    table="INV_MANUFACTURER"
>
    <id
        name="icManufacturer"
        type="java.math.BigDecimal"
        column="IC_MANUFACTURER"
    >
        <generator class="assigned" />
    </id>
    <property
        name="itemNumber"
        type="java.lang.String"
        column="ITEM_NUMBER"
        length="30"
    />
    <property
        name="vendorId"
        type="java.lang.String"
        column="VENDOR_ID"
        length="15"
    />
    <property
        name="partNumber"
        type="java.lang.String"
        column="PART_NUMBER"
        length="30"
    />
    <property
        name="notes"
        type="java.lang.String"
        column="NOTES"
        length="2000"
    />
    <property
        name="dateEntered"
        type="java.sql.Timestamp"
        column="DATE_ENTERED"
        length="7"
    />
    <property
        name="owner"
        type="java.lang.String"
        column="OWNER"
        length="15"
    />

    <!-- associations -->

</class>
</hibernate-mapping>
