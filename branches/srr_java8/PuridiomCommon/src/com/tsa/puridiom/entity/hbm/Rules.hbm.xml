<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

<!--
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class
    name="com.tsa.puridiom.entity.Rules"
    table="RULES"
>
    <id
        name="icRule"
        type="java.math.BigDecimal"
        column="IC_RULE"
    >
        <generator class="assigned" />
    </id>
    <property
        name="ruleName"
        type="java.lang.String"
        column="RULE_NAME"
        length="40"
    />
    <property
        name="ruleOrder"
        type="java.lang.String"
        column="RULE_ORDER"
        length="5"
    />
    <property
        name="ruleExpression"
        type="java.lang.String"
        column="RULE_EXPRESSION"
        length="2000"
    />
    <property
        name="validationMessage"
        type="java.lang.String"
        column="VALIDATION_MESSAGE"
        length="255"
    />
    <property
        name="validationSeverity"
        type="java.lang.String"
        column="VALIDATION_SEVERITY"
        length="1"
    />
    <property
        name="validationLink"
        type="java.lang.String"
        column="VALIDATION_LINK"
        length="255"
    />
    <property
        name="fieldName"
        type="java.lang.String"
        column="FIELD_NAME"
        length="60"
    />
    <property
        name="enabled"
        type="java.lang.String"
        column="ENABLED"
        length="1"
    />
    <property
        name="moduleAccess"
        type="java.lang.String"
        column="MODULE_ACCESS"
        length="30"
    />
    <property
        name="status"
        type="java.lang.String"
        column="STATUS"
        length="4"
    />
    <property
        name="owner"
        type="java.lang.String"
        column="OWNER"
        length="15"
    />
    <property
        name="lastChangeBy"
        type="java.lang.String"
        column="LAST_CHANGE_BY"
        length="15"
    />
    <property
        name="lastChangeDate"
        type="java.sql.Timestamp"
        column="LAST_CHANGE_DATE"
    />
    <!-- associations -->

</class>
</hibernate-mapping>
