<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="com.tsa.puridiom.entity.StdAttachment" 
    table="STD_ATTACHMENT"
>
    <id
        name="docIc"
        type="java.math.BigDecimal"
        column="DOC_IC"
    >
        <generator class="assigned" />
    </id>
    <property
        name="docTitle"
        type="java.lang.String"
        column="DOC_TITLE"
        length="128"
    />
    <property
        name="docFilename"
        type="java.lang.String"
        column="DOC_FILENAME"
        length="20"
    />
    <property
        name="docPrint"
        type="java.lang.String"
        column="DOC_PRINT"
        length="1"
    />

    <!-- associations -->

</class>
</hibernate-mapping>
