<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

<!--
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class
    name="com.tsa.puridiom.entity.Subsidiary"
    table="SUBSIDIARY"
>

     <id
        name="icHeader"
        type="java.math.BigDecimal"
        column="IC_HEADER"
    >
        <generator class="assigned" />
    </id>

    <property
        name="countryCode"
        type="java.lang.String"
        column="COUNTRY_CODE"
        length="5"
    />
    <property
        name="subsidiaryLanguaje"
        type="java.lang.String"
        column="SUBSIDIARY_LANGUAJE"
        length="15"
    />
    <property
        name="subsidiaryName"
        type="java.lang.String"
        column="SUBSIDIARY_NAME"
        length="255"
    />
    <property
        name="status"
        type="java.lang.String"
        column="STATUS"
        length="4"
    />

    <!-- associations -->

</class>
</hibernate-mapping>
