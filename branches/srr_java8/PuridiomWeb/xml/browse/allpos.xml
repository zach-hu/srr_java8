		<PoHeader_requisitionerCode>
			<label>brw-requisitioner</label>
			<detail>Y</detail>
			<size>50</size>
			<type>USER-ID</type>
		</PoHeader_requisitionerCode>
		<PoHeader_departmentCode>
			<label>brw-department</label>
			<detail>Y</detail>
			<size>25</size>
		</PoHeader_departmentCode>
    <PoHeader_internalComments>
      <label>brw-purpose</label>
      <detail>Y</detail>
    </PoHeader_internalComments>
    <PoHeader_expirationDate>
      <label>brw-dateExpires</label>
      <type>DATE</type>
      <size>10</size>
      <visible>0</visible>
    </PoHeader_expirationDate>
