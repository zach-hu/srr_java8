<browse_po_header>
  <columns>
    <PoHeader_icPoHeader>
          <hidden>Y</hidden>
          <visible>0</visible>
    </PoHeader_icPoHeader>
    <PoHeader_poNumber>
      <label>brw-poNumber</label>
      <link><![CDATA[javascript: viewPo('#PoHeader_icPoHeader^','#PoHeader_poType^'); void(0);]]></link>
      <size>10</size>
    </PoHeader_poNumber>
    <PoHeader_releaseNumber>
      <label>brw-releaseNumber</label>
      <type>STRING</type>
      <size>4</size>
