      <type>STRING</type>
      <size>4</size>
    </PoHeader_revisionNumber>
    <PoHeader_status>
      <label>brw-status</label>
      <type>STATUS</type>
      <size>15</size>
    </PoHeader_status>
    <PoHeader_poDate>
      <label>brw-poDate</label>
      <type>DATE</type>
      <size>10</size>
    </PoHeader_poDate>
    <PoHeader_buyerCode>
      <label>brw-buyer</label>
            <size>12</size>
            <type>USER-ID</type>
