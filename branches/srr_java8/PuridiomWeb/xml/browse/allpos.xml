    <PoHeader_total>
      <label>brw-total</label>
      <size>8</size>
      <type>DollarDecimal</type>
    </PoHeader_total>
	  <PoHeader_vendorId>
			<label>brw-vendorId</label>
			<detail>Y</detail>
			<size>40</size>
