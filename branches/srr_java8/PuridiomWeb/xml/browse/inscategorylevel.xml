<browse_ins_category_level>
	<columns>
		<InsCategoryLevel_iclLevel>
			<label>brw-iclLevel</label>
			<sort>A</sort>
			<link><![CDATA[javascript: returnMe('#InsCategoryLevel_iclLevel^'); void(0);]]></link>
			<size>10</size>
		</InsCategoryLevel_iclLevel>
		<InsCategoryLevel_iclDescription>
			<label>brw-iclDescription</label>
			<size>50</size>
		</InsCategoryLevel_iclDescription>
		<InsCategoryLevel_status>
			<label>brw-status</label>
			<type>STATUS</type>
			<size>10</size>
		</InsCategoryLevel_status>
		<InsCategoryLevel_owner>
			<label>brw-owner</label>
			<size>20</size>
			<type>USER-ID</type>
		</InsCategoryLevel_owner>
	</columns>
<sqlfrom>InsCategoryLevel as InsCategoryLevel</sqlfrom>
<sqlwhere><![CDATA[ (InsCategoryLevel.status = '02' OR (InsCategoryLevel.status = '01' AND InsCategoryLevel.dateExpires >= :as_today)) ]]></sqlwhere>
<sqlorderby>InsCategoryLevel.iclLevel ASC</sqlorderby>
<pagesize>15</pagesize>
<title>Insurance Category Level Browse</title>
</browse_ins_category_level>