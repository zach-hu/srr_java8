	<sqlfrom>RequisitionLine as RequisitionLine</sqlfrom>
	<sqlwhere><![CDATA[(RequisitionLine.status = '5155') AND (RequisitionLine.reqType = 'I')  ]]></sqlwhere>
	<sqlorderby default="Y">RequisitionLine.requisitionNumber ASC, RequisitionLine.lineNumber ASC</sqlorderby>
