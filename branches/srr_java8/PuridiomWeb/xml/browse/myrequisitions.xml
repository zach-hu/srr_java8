<browse_requisition_header>
	<columns>
		<RequisitionHeader_icReqHeader>
	        <hidden>Y</hidden>
		</RequisitionHeader_icReqHeader>
		<RequisitionHeader_requisitionNumber>
			<label><![CDATA[brw-requisitionNumber]]></label>
			<sort>D</sort>
			<link><![CDATA[javascript: viewReq('#RequisitionHeader_icReqHeader^','#RequisitionHeader_requisitionType^'); void(0);]]></link>
			<size>12</size>
		</RequisitionHeader_requisitionNumber>
		<RequisitionHeader_requisitionDate>
			<label><![CDATA[brw-requisitionDate]]></label>
			<size>10</size>
			<type>DATE</type>
		</RequisitionHeader_requisitionDate>
		<RequisitionHeader_dateEntered>
			<label><![CDATA[brw-dateentered]]></label>
			<hidden>Y</hidden>
			<type>DATE</type>
		</RequisitionHeader_dateEntered>
		<RequisitionHeader_requiredDate>
			<label><![CDATA[brw-requiredDate]]></label>
			<size>10</size>
			<type>DATE</type>
		</RequisitionHeader_requiredDate>
		<RequisitionHeader_requisitionerCode>
			<label><![CDATA[brw-requisitioner]]></label>
            <size>20</size>
            <type>USER-ID</type>
		</RequisitionHeader_requisitionerCode>
		<RequisitionHeader_departmentCode>
			<label><![CDATA[brw-department]]></label>
            <hidden>Y</hidden>
			<allow-filter>Y</allow-filter>
		</RequisitionHeader_departmentCode>
		<RequisitionHeader_vendorId>
			<label><![CDATA[brw-vendorId]]></label>
			<size>40</size>
			<detail>Y</detail>
		</RequisitionHeader_vendorId>
		<RequisitionHeader_fiscalYear>
			<label><![CDATA[brw-fiscalYear]]></label>
			<visible>0</visible>
			<allowfilter>Y</allowfilter>
		</RequisitionHeader_fiscalYear>
		<RequisitionHeader_shipToCode>
			<label><![CDATA[brw-shipToCode]]></label>
			<allowfilter>Y</allowfilter>
			<size>18</size>
		</RequisitionHeader_shipToCode>
		<RequisitionHeader_status>
			<label><![CDATA[brw-status]]></label>
			<type>STATUS</type>
			<size>15</size>
		</RequisitionHeader_status>
		<RequisitionHeader_total>
			<label><![CDATA[brw-total]]></label>
			<type>DollarDecimal</type>
			<size>15</size>
		</RequisitionHeader_total>
		<RequisitionHeader_approved>
			<label><![CDATA[brw-appDate]]></label>
			<hidden>Y</hidden>
		</RequisitionHeader_approved>
		<RequisitionHeader_owner>
			<label><![CDATA[brw-owner]]></label>
	        <hidden>Y</hidden>
			<allowfilter>Y</allowfilter>
		</RequisitionHeader_owner>
		<RequisitionHeader_vendorName>
			<label><![CDATA[brw-vendorName]]></label>
			<size>60</size>
			<detail>Y</detail>
		</RequisitionHeader_vendorName>
		<RequisitionHeader_buyer>
			<label><![CDATA[brw-suggestedBuyer]]></label>
			<detail>Y</detail>
			<size>40</size>
			<type>USER-ID</type>
		</RequisitionHeader_buyer>
		<RequisitionHeader_timeZone>
			<label><![CDATA[brw-timezone]]></label>
			<detail>Y</detail>
			<size>60</size>
			<type>TIMEZONE</type>
		</RequisitionHeader_timeZone>
		<RequisitionHeader_internalComments>
			<label><![CDATA[brw-req-purpose]]></label>
            <size>100</size>
            <detail>Y</detail>
		</RequisitionHeader_internalComments>
	</columns>
<sqlfrom>RequisitionHeader as RequisitionHeader</sqlfrom>
<sqlwhere><![CDATA[(RequisitionHeader.owner = :as_userid OR RequisitionHeader.requisitionerCode = :as_userid)
					AND (RequisitionHeader.requisitionNumber <> 'N/A')
]]></sqlwhere>
<sqlorderby>RequisitionHeader.requisitionNumber DESC</sqlorderby>
<pagesize>15</pagesize>
<title>Requisition Browse</title>
</browse_requisition_header>

