<browse_po_header>
	<columns>
		<PoHeader_icPoHeader>
	        <hidden>Y</hidden>
	        <visible>0</visible>
		</PoHeader_icPoHeader>
		<PoHeader_poNumber>
			<label>brw-poNumber</label>
			<sort>D</sort>
			<size>15</size>
		</PoHeader_poNumber>
		<PoHeader_releaseNumber>
			<label>brw-releaseNumber</label>
			<type>STRING</type>
			<size>5</size>
		</PoHeader_releaseNumber>
		<PoHeader_revisionNumber>
			<label>brw-revisionNumber</label>
			<type>STRING</type>
			<size>5</size>
		</PoHeader_revisionNumber>
		<PoHeader_status>
			<label>brw-status</label>
			<type>STATUS</type>
			<size>20</size>
		</PoHeader_status>
		<PoHeader_vendorRating>
			<label>brw-rating</label>
			<type>EVALUATION-IMG</type>
			<size>20</size>
		</PoHeader_vendorRating>
		<PoHeader_total>
			<label>brw-total</label>
			<size>15</size>
			<type>DollarDecimal</type>
		</PoHeader_total>
		<PoHeader_internalComments>
			<label>purpose</label>
			<detail>Y</detail>
			<size>100</size>
		</PoHeader_internalComments>
	</columns>
<sqlfrom>PoHeader as PoHeader</sqlfrom>
<sqlwhere><![CDATA[PoHeader.rated = 'Y'  ]]></sqlwhere>
<sqlorderby>PoHeader.poNumber DESC, PoHeader.releaseNumber DESC</sqlorderby>
<pagesize>15</pagesize>
<title>Purchasing Browse</title>
<detail-visible>Y</detail-visible>
</browse_po_header>