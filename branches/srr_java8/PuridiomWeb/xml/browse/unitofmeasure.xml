<browse_unitofmeasure>
	<columns>
		<UnitOfMeasure_umCode>
			<label>brw-uom</label>
			<sort>A</sort>
			<link><![CDATA[javascript: returnMe('#UnitOfMeasure_umCode^'); void(0);]]></link>
		</UnitOfMeasure_umCode>
		<UnitOfMeasure_description>
			<label>description</label>
		</UnitOfMeasure_description>
		<UnitOfMeasure_factor>
			<label>brw-factor</label>
			<hidden-input>Y</hidden-input>
		</UnitOfMeasure_factor>
	</columns>
<sqlfrom>UnitOfMeasure as UnitOfMeasure</sqlfrom>
<sqlwhere><![CDATA[ (UnitOfMeasure.status = '02' OR (UnitOfMeasure.status = '01' AND UnitOfMeasure.dateExpires >= :as_today)) ]]></sqlwhere>
<sqlorderby>UnitOfMeasure.umCode ASC</sqlorderby>
<pagesize>15</pagesize>
<title>Units of Measure Browse</title>
</browse_unitofmeasure>