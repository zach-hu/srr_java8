<sqlfrom>Contact as Contact, Address as Address, Vendor as Vendor, VendorAffiliate as VendorAffiliate</sqlfrom>
<sqlwhere><![CDATA[ (Contact.id.vendorId = Address.id.addressType
	AND Contact.addressCode = Address.id.addressCode
	AND Contact.id.contactType = 'DEFAULT'
	AND Contact.status <> '03'
	AND Vendor.vendorId = Contact.id.vendorId
	AND Vendor.vendorType = 'A'
	AND VendorAffiliate.id.affiliateId = Vendor.vendorId
	AND NOT EXISTS ( select PO.vendorId from PoHeader as PO
		where VendorAffiliate.id.affiliateId = PO.vendorId 
			and PO.status < '9000'
			and PO.expirationDate >= :as_today))
	) ]]>
</sqlwhere>
<sqlorderby>Vendor.vendorId ASC</sqlorderby>
<pagesize>10</pagesize>
<title>Affiliate Browse</title>
</browse_vendor-affiliates>

