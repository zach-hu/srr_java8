<?xml version="1.0" encoding="UTF-8"?>
<process name="inspectionGetFormNumber" connection-type="Transaction-Required">
	<activities>
		<activity synchronous="true" continue-on-failure="false" name="getNumberSetup">
			<task synchronous="true" continue-on-failure="false" name="inspectionHeader">
				<rule-filename>is-inspectionheader-empty.xml</rule-filename>
				<task-object-name>com.tsa.puridiom.inspectionheader.tasks.InspectionHeaderRetrieveById</task-object-name>
				<post-action></post-action>
			</task>
			<task synchronous="true" continue-on-failure="false" name="inspectionGetNumberSetup">
				<task-object-name>com.tsa.puridiom.inspection.tasks.InspectionGetNumberSetup</task-object-name>
			</task>
		</activity>
		<activity synchronous="true" continue-on-failure="false" name="autoGeneratedNumber">
			<task synchronous="true" continue-on-failure="false" name="inspectionGenerateNumber">
				<rule-filename>is-inspection-number-autogen-required.xml</rule-filename>
				<task-object-name>com.tsagate.foundation.processengine.InvokePuridiomProcessTask</task-object-name>
				<process-filename>inspection-generate-form-number.xml</process-filename>
			</task>
			<task synchronous="true" continue-on-failure="false" name="InspectionHeader_inspectionNumber">
				<task-object-name>com.tsa.puridiom.inspection.tasks.InspectionFormatDocNumber</task-object-name>
			</task>
			<task synchronous="true" continue-on-failure="false" name="inspectionHeaderUpdate">
				<task-object-name>com.tsagate.foundation.processengine.InvokePuridiomProcessTask</task-object-name>
				<process-filename>inspectionheader-update.xml</process-filename>
			</task>
		</activity>
	</activities>
</process>