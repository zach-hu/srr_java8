<catalogitem_register>
	<columns>
		<CatalogItem_catalogId>
			<label>Catalog Id</label>
		</CatalogItem_catalogId>
		<CatalogItem_itemNumber>
			<label>Item Number</label>
		</CatalogItem_itemNumber>
		<CatalogItem_commodity>
			<label>Commodity</label>
		</CatalogItem_commodity>
		<CatalogItem_cost>
			<label>Cost</label>
		</CatalogItem_cost>
		<CatalogItem_umCode>
			<label>Um Code</label>
		</CatalogItem_umCode>
		<CatalogItem_description>
			<label>Description</label>
		</CatalogItem_description>
		<CatalogItem_mfgName>
			<label>Manufacturer</label>
		</CatalogItem_mfgName>
		<CatalogItem_kit>
			<label>Kit Item</label>
		</CatalogItem_kit>
		<CatalogItem_modelNumber>
			<label>Model Number</label>
		</CatalogItem_modelNumber>
	</columns>
	<sqlfrom>CatalogItem as CatalogItem</sqlfrom>
	<sqlorderby>CatalogItem.id.itemNumber ASC</sqlorderby>
	<sqlwhere><![CDATA[(CatalogItem.status <> '03')]]></sqlwhere>
	<title>Catalog Items Register Report</title>
</catalogitem_register>
