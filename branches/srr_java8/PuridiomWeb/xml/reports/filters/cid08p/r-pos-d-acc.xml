  <sqlfrom>PoHeader as PoHeader, PoLine as PoLine, RequisitionHeader as RequisitionHeader, Account as Account,</sqlfrom>
  <sqlorderby>PoHeader.poNumber ASC</sqlorderby>
  <sqlwhere><![CDATA[
  	( PoLine.icAccount = Account.id.icLine )  and 
	( PoHeader.icPoHeader = PoLine.icPoHeader ) and
	( RequisitionHeader.requisitionNumber = PoHeader.requisitionNumber ) and
	( PoHeader.lastRevision = 'C' ) and ( PoHeader.poDate >= :as_baseReportDate ) and
	( PoHeader.status > '3020' ) and
	( PoHeader.status < '9020' )
	]]></sqlwhere>
  <title>PO Accrual Report (Detail)</title>
</r-pos-d-acc>

