<r-pos-s-iph>
  <columns>
		<PoHeader_poNumber>
			<label>po-number</label>
			<type>STRING</type>
			<visible>1</visible>
		</PoHeader_poNumber>
		<PoHeader_poDate>
			<label>orderDate</label>
			<type>DATE</type>
			<visible>1</visible>
		</PoHeader_poDate>
		<PoHeader_buyerCode>
			<label>Procurement Officer Code</label>
			<type>STRING</type>
			<visible>1</visible>
		</PoHeader_buyerCode>
		<UserProfile_firstName>
			<label>First Name</label>
			<type>STRING</type>
			<visible>1</visible>
		</UserProfile_firstName>
		<UserProfile_lastName>
			<label>Last Name</label>
			<type>STRING</type>
			<visible>1</visible>
		</UserProfile_lastName>
		<PoHeader_vendorId>
			<label>Supplier Code</label>
			<type>STRING</type>
			<visible>1</visible>
		</PoHeader_vendorId>
		<Vendor_vendorName>
			<label>Supplier Name</label>
			<type>STRING</type>
			<visible>1</visible>
		</Vendor_vendorName>
		<PoLine_status>
			<label>Status</label>
			<type>STATUS</type>
			<visible>1</visible>
		</PoLine_status>
		<PoLine_itemNumber>
			<label>Item Number</label>
			<type>STRING</type>
			<visible>1</visible>
		</PoLine_itemNumber>
		<PoLine_description>
			<label>Description</label>
			<type>STRING</type>
			<visible>1</visible>
		</PoLine_description>
		<PoLine_quantity>
			<label>Quantity</label>
			<type>BIGDECIMAL</type>
			<visible>1</visible>
		</PoLine_quantity>
		<PoLine_unitPrice>
			<label>Unit Price</label>
			<type>BIGDECIMAL</type>
			<visible>1</visible>
		</PoLine_unitPrice>
		<PoLine_lineTotal>
			<label>Total</label>
			<type>BIGDECIMAL</type>
			<visible>1</visible>
		</PoLine_lineTotal>
		<PoLine_mfgName>
			<label>Mfg Name</label>
			<type>STRING</type>
			<visible>1</visible>
		</PoLine_mfgName>
		<PoHeader_currencyCode>
			<label>Currency Code</label>
			<type>STRING</type>
			<visible>1</visible>
		</PoHeader_currencyCode>
		<PoHeader_currencyFactor>
			<label>Currency Factor</label>
			<type>BIGDECIMAL</type>
			<visible>1</visible>
		</PoHeader_currencyFactor>
		<PoHeader_fiscalYear>
			<label>Fiscal Year</label>
			<type>STRING</type>
			<visible>1</visible>
		</PoHeader_fiscalYear>
	</columns>
  <sqlfrom> PoHeader as  PoHeader , PoLine as PoLine , UserProfile as UserProfile , Vendor as Vendor </sqlfrom>
  <sqlwhere><![CDATA[
  	( PoHeader.status > '3020' ) and
  	( PoHeader.status < '9020' ) and ( PoHeader.poDate >= :as_baseReportDate ) and
  	( PoHeader.icPoHeader = PoLine.icPoHeader ) and
  	( PoHeader.lastRevision = 'C' ) and
  	( PoHeader.buyerCode = UserProfile.userId ) and
  	( PoHeader.vendorId = Vendor.vendorId ) and
  	]]></sqlwhere>
  <sqlorderby>PoLine.itemNumber</sqlorderby>
  <title>Item - Purchase History Report</title>
</r-pos-s-iph>