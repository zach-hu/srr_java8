<cost-center-approvals>
	<columns>
		<UserProfile_userId>
			<label>User ID</label>
		</UserProfile_userId>
		<UserProfile_firstName>
			<label>First Name</label>
		</UserProfile_firstName>
		<UserProfile_lastName>
			<label>Last Name</label>
		</UserProfile_lastName>
		<AppRule_udf1Code>
			<label>Cost Center</label>
		</AppRule_udf1Code>
		<AppRule_udf2Code>
			<label>Budgeted</label>
		</AppRule_udf2Code>
		<AppRule_udf3Code>
			<label>Capital</label>
		</AppRule_udf3Code>
		<AppRule_udf4Code>
			<label>Designated Approver</label>
		</AppRule_udf4Code>
	</columns>
<sqlfrom>UserProfile as UserProfile, AppRule as AppRule</sqlfrom>
<sqlwhere><![CDATA[UserProfile.userId = AppRule.userId AND UserProfile.status <> '03']]></sqlwhere>
<title>Cost Center Approvals Report</title>
</cost-center-approvals>