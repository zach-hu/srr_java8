	<sqlfrom>PoHeader as PoHeader, PoLine as PoLine, Vendor as Vendor</sqlfrom>
	<sqlwhere>(PoHeader.icPoHeader = PoLine.icPoHeader) and (PoHeader.vendorId = Vendor.vendorId) and (PoHeader.poNumber &lt;&gt; 'N/A') and (( PoHeader.lastRevision = 'C' ) and ( PoHeader.status &lt;&gt; '9010' ) and (PoHeader.poType &lt;&gt; 'NO'))</sqlwhere>
	<sqlorderby>PoHeader.poNumber, PoHeader.releaseNumber, PoHeader.revisionNumber, PoLine.lineNumber</sqlorderby>
	<pagesize>15</pagesize>
