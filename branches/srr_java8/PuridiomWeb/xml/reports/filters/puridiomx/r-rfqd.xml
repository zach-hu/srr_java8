<sqlfrom> RfqHeader as RfqHeader, RfqLine as RfqLine </sqlfrom>
<sqlwhere><![CDATA[
RfqHeader.icRfqHeader = RfqLine.icRfqHeader and
RfqLine.status > '2000' and
RfqLine.status < '9020'
]]></sqlwhere>
<title>RFQ Register Report</title>
</r-rfqd>
