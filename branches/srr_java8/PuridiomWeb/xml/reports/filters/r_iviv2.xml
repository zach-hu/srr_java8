<r_iviv2>
	
  <columns>
		<InvBinLocation_itemLocation>
			<label>Item Location</label>
			<type>STRING</type>
		</InvBinLocation_itemLocation>
		<InvItem_description>
			<label>Description</label>
			<type>STRING</type>
		</InvItem_description>
		<InvItem_commodity>
			<label>Commodity</label>
			<type>STRING</type>
		</InvItem_commodity>
		<InvBinLocation_qtyOnHand>
			<label>Qty On Hand</label>
			<type>BIGDECIMAL</type>
		</InvBinLocation_qtyOnHand>
		<InvItem_udf1Code>
			<label>Udf 1 Code</label>
			<type>STRING</type>
		</InvItem_udf1Code>
		<InvItem_udf2Code>
			<label>Udf 2 Code</label>
			<type>STRING</type>
		</InvItem_udf2Code>
		<InvItem_udf3Code>
			<label>Udf 3 Code</label>
			<type>STRING</type>
		</InvItem_udf3Code>
		<InvItem_udf4Code>
			<label>Udf 4 Code</label>
			<type>STRING</type>
		</InvItem_udf4Code>
		<InvItem_udf5Code>
			<label>Udf 5 Code</label>
			<type>STRING</type>
		</InvItem_udf5Code>
		<InvItem_itemNumber>
			<label>Item Number</label>
			<type>STRING</type>
		</InvItem_itemNumber>
		<InvItem_unitOfIssue>
			<label>Unit Of Issue</label>
			<type>STRING</type>
		</InvItem_unitOfIssue>
		<InvBinLocation_physActual>
			<label>Phys Actual</label>
			<type>BIGDECIMAL</type>
		</InvBinLocation_physActual>
		<InvBinLocation_physOriginal>
			<label>Phys Original</label>
			<type>BIGDECIMAL</type>
		</InvBinLocation_physOriginal>
	</columns>
	
  <sqlfrom> InvItem as InvItem , InvBinLocation as InvBinLocation </sqlfrom>
	
  <sqlwhere><![CDATA[(InvItem.itemNumber = InvBinLocation.itemNumber)]]></sqlwhere>
	
  <title>InvBinLocation Physical Count Report</title>

</r_iviv2>

