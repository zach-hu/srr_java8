			<type>STRING</type>
		</InvBinLocation_itemLocation>
		<InvBinLocation_aisle>
			<label>Aisle</label>
			<type>STRING</type>
		</InvBinLocation_aisle>
		<InvBinLocation_locrow>
			<label>Locrow</label>
			<type>STRING</type>
		</InvBinLocation_locrow>
		<InvBinLocation_tier>
			<label>Tier</label>
			<type>STRING</type>
		</InvBinLocation_tier>
		<InvBinLocation_bin>
