	<sqlfrom>PoHeader as PoHeader, Account as Account, Address as Address, Vendor as Vendor</sqlfrom>
	<sqlwhere>
		<![CDATA[( PoHeader.contactAddr = Address.id.addressCode) and ( PoHeader.vendorId = Vendor.vendorId ) and ( PoHeader.icPoHeader = Account.id.icHeader ) and  ( ( PoHeader.status >= '3030' ) AND  ( PoHeader.status < '9000' ) AND( PoHeader.approved = 'Y' ) ) ]]>
	</sqlwhere>
	<sqlorderby>PoHeader.vendorId</sqlorderby>
	<title>Supplier Purchases Report</title>
