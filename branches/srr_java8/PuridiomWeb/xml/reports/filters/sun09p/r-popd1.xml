	<sqlfrom>PoHeader as PoHeader, PoLine as PoLine, Account as Account</sqlfrom>
	<sqlwhere><![CDATA[( PoHeader.icPoHeader = PoLine.icPoHeader ) and PoLine.icAccount = Account.id.icLine and PoHeader.icPoHeader = Account.id.icHeader and
	  ( ( PoHeader.lastRevision = 'C' ) and  ( PoHeader.status > '3020') and  (PoHeader.status < '9020')) and (PoHeader.poDate >= :as_baseReportDate)]]></sqlwhere>
	<sqlorderby>PoHeader.poNumber ASC, PoHeader.releaseNumber ASC, PoLine.lineNumber ASC</sqlorderby>
 	<title>PO Payment Detail Report</title>
