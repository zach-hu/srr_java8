<?xml version="1.0" encoding="UTF-8"?>
<validation-rules>
	<validations>
		<save enabled="yes">
			<rule>
				<filename>user-email-m.xml</filename>
				<alias>email</alias>
				<msg>You must enter a valid Email!</msg>
				<severity>E</severity>
				<order>100</order>
				<enabled>N</enabled>
				<fieldname>UserProfile_mailId</fieldname>
				<link><![CDATA[javascript: doSubmit('/admin/user/user_profile.jsp', 'UserProfile_mailId'); void(0);]]></link>
			</rule>
		</save>
	</validations>
</validation-rules>