<?xml version="1.0" encoding="UTF-8"?>
<validation-rules>
	<validations>
    <save enabled="yes">
    	<rule>
        	<filename>amount-compare-total.xml</filename>
			<alias>Compare Total</alias>
			<msg>Invoice Amount must equal Allocated Amount!</msg>
			<severity>E</severity>
			<order>10</order>
			<enabled>Y</enabled>
			<link><![CDATA[javascript: doSubmit('invoice/iv_review.jsp', 'InvoiceRetrieve'); void(0);]]></link>
      	</rule>
      	<rule>
        	<filename>inv-rq01-mv.xml</filename>
			<alias>Compare udf1</alias>
			<msg>Invoice UDF 1 not is valid!</msg>
			<severity>E</severity>
			<order>11</order>
			<enabled>Y</enabled>
			<link><![CDATA[javascript: doSubmit('invoice/iv_general_info.jsp', 'InvoiceHeader_udf1Code'); void(0);]]></link>
    	</rule>
    </save>
  </validations>
</validation-rules>